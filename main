from PyQt5 import QtWidgets
from PyQt5.QtWidgets import QApplication, QMainWindow

import sys

def runButton():
    print('У меня у самого нет(')

def runButton2():
    print('Крутой')


def mainApp():
    app = QApplication(sys.argv)
    windows = QMainWindow()

    windows.setWindowTitle('ЛолКекЧебурек')
    windows.setGeometry(500, 100, 1000, 800)


    lol = QtWidgets.QLabel(windows)
    lol.setText('Хочешь Чебурек?')
    lol.move(450, 200)

    kek = QtWidgets.QPushButton(windows)
    kek.move(450, 170)
    kek.setText('Получить')
    kek.setFixedWidth(300)
    kek.clicked.connect(runButton)

    chebyr = QtWidgets.QPushButton(windows)
    chebyr.move(600, 240)
    chebyr.setText('Узнай кто ты?')
    chebyr.setFixedWidth(300)
    chebyr.clicked.connect(runButton2)


    windows.show()
    sys.exit(app.exec_())

runButton2()
runButton()
mainApp()
